<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:hola="http://www.solmix.org/schema/hola"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
 xsi:schemaLocation="http://www.springframework.org/schema/beans 
					http://www.springframework.org/schema/beans/spring-beans.xsd
					http://www.solmix.org/schema/hola 
					../../../main/resources/schema/hola-1.0.xsd ">
					
	<!-- 
	  id="app" 
	  name="monitor-app"
	  version="1.0.0"
	  owner="solmix"
	  organization="solmix.org"
	  architecture="modle"
	  environment="run" dev:开发,test:测试,run:运行
	  discovery="d1,d2"
	  monitor="monitor1"
	 -->		
	<hola:application id="app" 
					  name="monitor-app"
					  version="1.0.0"
					  owner="solmix"
					  organization="solmix.org"
					  architecture="modle"
					  environment="run"
					  discovery="d1,d2"
					  monitor="monitor1"/>
					  
	<hola:module id="module"
				 name="monitor-app"
				 version="1.0.0"
				 owner="solmix"
				 organization="solmix.org"
				 discovery="d1,d2"
				 monitor="monitor1"/>
				 
	<!-- 
	dynamic zk声明为动态时,连接断开注册失效
	 -->
	<hola:discovery id="discovery"
					name="zk-discovery"
					version="1.0.1"
					protocol="hola"
					port="1314"
					address="localhost:2181,localhost:2182"
					dynamic="true"
					timeout="5000"
					local-file="/tmp/discovery/file"
					/>
					<!-- 
					group:服务器分组
					 -->
	<hola:server id="server"
				 host="localhost"
				 port="2342"
				 version="1.0.0"
				 group="group1"
				 >
	
	</hola:server>
					
 </beans>
