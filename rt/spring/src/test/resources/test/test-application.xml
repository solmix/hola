<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:hola="http://www.solmix.org/schema/hola"
	xmlns:c="http://www.solmix.org/schema/rt"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
						http://www.springframework.org/schema/beans/spring-beans.xsd
						http://www.solmix.org/schema/hola 
						http://www.solmix.org/schema/hola/hola.xsd
						http://www.solmix.org/schema/rt 
						http://www.solmix.org/schema/rt/rt.xsd">


	 <hola:application name="test" 
		version="1.0.2" 
		owner="solmix"
		organization="solmix.org" 
		architecture="linux" 
		environment="test"
		discovery="discovery"
		default="true" /> 
		<!-- id -->
	 <hola:application  id="test2"/> 
	 <c:container name="solmix3"/>
	 
	<hola:module name="module" 
		version="1.0.2" 
		owner="solmix"
		organization="solmix.org" 
		default="true" 
		discovery="discovery,discovery2"/>
	<hola:module id="module2" />
	
	<hola:monitor name="monitor" 
		address="hola://localhost:9812" 
		username="hola"
		password="hola" 
		group="default"
		version="1.0.0"
		default="true" >
		<hola:properties>                                                                       
			<entry key="key" value="value" />
		</hola:properties>
		
		</hola:monitor> 
		
	<hola:discovery name="discovery"
		address="zk://localhost:2340"
		username="hola"
		password="hola"
		protocol="zk"
		port="2340"
		cluster="true"
		group="default"
		version="1.0.0"
		timeout="5000"
		check="false"
		dynamic="true"
		advertise="true"
		subscribe="true"
		file="/home/solmix/a"
		default="true">
		<hola:properties>
			<entry key="key" value="value2" />
		</hola:properties>
		</hola:discovery>
		
		<hola:discovery name="discovery2"
		address="zk://localhost:2340"
		username="hola"
		password="hola">
		
		</hola:discovery>
		
		<!-- server definition -->
	<hola:server id="server"
		timeout="5000"
		retries="3"
		actives="200"
		loadbalance="robin"
		async="true"
		asyncwait="false"
		connections="100"
		protocol="hola"
		proxy="jdk"
		cluster="failover"
		scope="local"
		discovery="discovery"
		application="test"
		module="module"
		monitor="monitor"
		version="1.0.9"
		group="group1"
		weight="9999"
		document="http://localhost/docs/docserver.html"
		dynamic="true"
		executes="1000"
		publish="true"
		host="localhost"
		port="1314"
		contextpath="/slx"
		executor="fixed"
		threads="188"
		iothreads="20"
		queues="89"
		accepts="45"
		codec="hola"
		serial="jdk"
		charset="utf-8"
		payload="8096"
		heartbeat="6000"
		transporter="netty"
		buffer="10000"
		exchanger="generic"
		dispatcher="normal"
		advertise="true"
		networker="default"
		default="true">
		
		<hola:service id="innerService"
			interface="org.solmix.hola.rt.spring.HelloService"
			ref="hello">
		</hola:service>
		<hola:service id="innerService2"
			interface="org.solmix.hola.rt.spring.HelloService"
			ref="hello">
		</hola:service>
		<hola:properties>
			<entry key="key1" value="value1" />
		</hola:properties>
	</hola:server>
	<bean id="hello" class="org.solmix.hola.rt.spring.HelloServiceImpl"></bean>
	<hola:service id="service"
		timeout="5000"
		retries="3"
		actives="200"
		loadbalance="robin"
		async="true"
		asyncwait="false"
		connections="100"
		protocol="hola"
		proxy="jdk"
		cluster="failover"
		scope="local"
		discovery="discovery"
		application="test"
		module="module"
		monitor="monitor"
		version="1.0.9"
		group="group1"
		weight="9999"
		document="http://localhost/docs/docserver.html"
		dynamic="true"
		executes="1000"
		publish="true"
		interface="org.solmix.hola.rt.spring.HelloService"
		delay="3000"
		path="/service"
		ref="hello"
		server="server"
		generic="false">
		<hola:properties>
			<entry key="key1" value="value1" />
		</hola:properties>
	</hola:service>
	
	
	<hola:service id="service2"
				  interface="org.solmix.hola.rt.spring.HelloService">
		<hola:ref>
			<bean class="org.solmix.hola.rt.spring.HelloServiceImpl"></bean>
		</hola:ref>
		<hola:methods>
			<hola:method name="sayhello">
				<hola:argument index="0"></hola:argument>
			</hola:method>
		</hola:methods>
	</hola:service> 
	
		<!-- client definition -->
	<hola:client id="client"
		timeout="5000"
		retries="3"
		actives="200"
		loadbalance="robin"
		async="true"
		asyncwait="false"
		connections="100"
		protocol="hola"
		proxy="jdk"
		cluster="failover"
		scope="local"
		discovery="discovery"
		application="test"
		module="module"
		monitor="monitor"
		version="1.0.9"
		group="group1"
		check="true"
		generic="true"
		reconnect="rec"
		lazy="true"
		default="true">
		<hola:properties>
			<entry key="key1" value="value1" />
		</hola:properties>
	</hola:client>
	<hola:reference id="reference"
		timeout="5000"
		retries="3"
		actives="200"
		loadbalance="robin"
		async="true"
		asyncwait="false"
		connections="100"
		protocol="hola"
		proxy="jdk"
		cluster="failover"
		scope="local"
		discovery="discovery"
		application="test"
		module="module"
		monitor="monitor"
		version="1.0.9"
		group="group1"
		check="true"
		generic="true"
		reconnect="rec"
		lazy="true"
	 client="client"
	 url="hola://localhost:1314"
	interface="org.solmix.hola.rt.spring.HelloService">
	<hola:properties>
			<entry key="key1" value="value1" />
		</hola:properties>
	</hola:reference>
</beans>
